@model IEnumerable<SachOnline.Models.KHACHHANG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}



<table class="table">
    <tr>
        <th>
            Họ Tên:
        </th>
        <th>
            Tài Khoản:
        </th>
        <th>
            Mật Khẩu:
        </th>
        <th>
            Email:
        </th>
        <th>
            Địa Chỉ:
        </th>
        <th>
            Số Điện Thoại:
        </th>
        <th>
            Ngày Sinh: 
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HoTen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Taikhoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Matkhau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiachiKH)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DienthoaiKH)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ngaysinh)
        </td>
        <td>
            <a href="@Url.Action("Edit", new { id = item.MaKH })" class="btn btn-warning"> Edit</a>
            <a href="@Url.Action("Details", new { id = item.MaKH })" class="btn btn-primary"> Details</a>
            <a href="@Url.Action("Delete", new { id = item.MaKH })" class="btn btn-danger"> Delete</a>
        </td>
    </tr>
}

</table>
